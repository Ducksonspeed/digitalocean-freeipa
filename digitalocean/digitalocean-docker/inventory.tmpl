[master]
%{ for index, name in master-name ~}
${name} ansible_host=${master-ip[index]} ansible_ssh_user=root ansible_ssh_private_key_file=~/.ssh/leaseweb 
%{ endfor ~}
[node]
%{ for index, name in worker-name ~}
${name} ansible_host=${worker-ip[index]} ansible_ssh_user=root ansible_ssh_private_key_file=~/.ssh/leaseweb 
%{ endfor ~}
[kube-cluster:children]
master
node
